<UserControl x:Class="EMS_2.Scheduling.AppointmentDetailLine"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:scheduling="clr-namespace:EMS_2.Scheduling"
             xmlns:appointmentDetails="clr-namespace:EMS_2.Scheduling.AppointmentDetail"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<Grid Margin="3" Name="grdAppointmentDetails" PreviewMouseLeftButtonDown="GrdAppointmentDetails_PreviewMouseLeftButtonDown" >
		<Rectangle Fill="LightGray" Name="background" />
		<Border BorderThickness="1" BorderBrush="Black" />
		<Label Name="text" />
	</Grid>
</UserControl>
